{
  "name": "@department-of-veterans-affairs/react-jsonschema-form",
  "version": "1.2.6",
  "description": "A simple React component capable of building HTML forms out of a JSON schema.",
  "scripts": {
    "build:readme": "toctoc README.md -w",
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.dist.js --mode production",
    "build:playground": "rimraf build && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --mode production && cp playground/index.prod.html build/index.html",
    "dist": "npm run build:lib && npm run build:dist",
    "lint": "eslint src test",
    "publish-to-gh-pages": "npm run build:playground && gh-pages --dist build/",
    "publish-to-npm": "npm run build:readme && npm run dist && npm publish",
    "start": "node devServer.js",
    "tdd": "cross-env NODE_ENV=test  mocha --compilers js:babel-register --watch --require ./test/setup-jsdom.js test/**/*_test.js",
    "test": " cross-env NODE_ENV=test   mocha --compilers js:babel-register --require ./test/setup-jsdom.js test/**/*_test.js"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "npm": "^2.14.7",
    "node": ">=14"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "jsonschema": "^1.4.1",
    "lodash.topath": "^4.5.2",
    "prop-types": "^15.8.1",
    "react-is": "^17.0.2",
    "setimmediate": "^1.0.5"
  },
  "devDependencies": {
    "atob": "^2.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "codemirror": "^5.65.17",
    "cross-env": "^2.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^4.3.0",
    "estraverse": "^5.3.0",
    "estraverse-fb": "^1.3.2",
    "express": "^4.19.2",
    "gh-pages": "^0.11.0",
    "html": "0.0.10",
    "jsdom": "^8.5.0",
    "mocha": "^2.5.3",
    "react": "^16.14.0",
    "react-codemirror2": "^8.0.0",
    "react-dom": "^16.14.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.6.0",
    "rimraf": "^2.7.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "toctoc": "^0.2.4",
    "webpack": "^4.47.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.4",
    "webpack-hot-middleware": "^2.26.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/react-jsonschema-form.git"
  },
  "author": "DSVA",
  "keywords": [
    "react",
    "form",
    "json-schema"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/department-of-veterans-affairs/react-jsonschema-form#readme"
}
